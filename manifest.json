{
	"manifest_version": 2,
	"name": "Tidal-keys",
	"version": "1.2",
	"background": {
		"scripts": [
			"background.js"
		],
		"persistent": true
	},
	"browser_action": {
		"default_title": "Tidal-keys"
	},
	"permissions": [
		"https://listen.tidal.com/"
	],
	"commands": {
		"play": {
			"description": "Play/Pause"
		},
		"previous": {
			"description": "Previous"
		},
		"next": {
			"description": "Next"
		}
	}
}
